<main role="main" class="container">

<h1>Games</h1>

<table class="table table-sm">
  <thead>
    <tr>
      <th scope="col">GM ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Game</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @gm_form2s.each do |gm_form2| %>
      <%if gm_form2.approved == true%>
        <tr>
            <td><%= gm_form2.id %></td>
            <td><%= gm_form2.first_name %></td>
            <td><%= gm_form2.last_name %></td>
            <td><%= gm_form2.games %></td>
        </tr>
      <%end%>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to 'Schedule', schedule_path %> <br>
  <%= link_to 'Home', home_path %> <br> 
</p>
</main><!-- /.container -->