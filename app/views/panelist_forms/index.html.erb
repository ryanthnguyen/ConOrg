<main role="main" class="container">

<h1>Panelist Forms</h1>

<table class="table table-sm">
  <thead>
    <tr>
      <th scope="col">Pen Name</th>
      <th scope="col">Title</th>
      <th scope="col">Email</th>
      <th scope="col">Availability</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% @panelist_forms.each do |panelist_form| %>
    <tr>
        <td><%= panelist_form.pen_name %></td>
        <td><%= panelist_form.title %></td>
        <td><%= panelist_form.email_address %></td>
        <td><%= panelist_form.availability %></td>
        <td><%= link_to 'show', panelist_form_path(panelist_form)%>
        <%= link_to 'edit', edit_panelist_form_path(panelist_form)%>
        <%= link_to 'destroy', panelist_form_path(panelist_form), method: :delete %>
        </td>
        
    </tr>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to 'New Panelist Form', new_panelist_form_path %> <br> 
</p>
</main><!-- /.container -->